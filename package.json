{
  "name": "banner_link_admin_panel",
  "version": "10.1.0",
  "description": "Banner and link administration panel",
  "main": "index.html",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:html": "globstar nu-html-checker *.html",
    "init-project": "npm install && npm-run-all init:*",
    "init:dirs": "mkdirp sass sass/components sass/pages sass/sections css vendor images images/icons js",
    "init:files": "touch README.md index.html styleguide.html sass/style.scss sass/_mixins.scss sass/_layout.scss sass/_normalize.scss sass/_variables.scss js/script.js css/style.css",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "npm-run-all build:* test",
    "build:clean": "rimraf css",
    "build:sass": "node-sass --output-style compact -o css sass",
    "build:autoprefixer": "autoprefixer-cli css/style.css",
    "build-dev:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "watch": "npm-run-all build -p watch:*",
    "watch:sassprefixer": "onchange -d 100 sass/*.scss sass/**/*.scss -- npm-run-all watch:sass watch:autoprefixer",
    "watch:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "watch:autoprefixer": "autoprefixer-cli css/style.css ",
    "watch:browsersync": "browser-sync start --server --files css/*.css *.html"
  },
  "keywords": [],
  "author": "Bartek Buga≈Ça",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.26.3",
    "rimraf": "^2.6.3",
    "globstar": "^1.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "nu-html-checker": "^0.1.0",
    "onchange": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://git@github.com:bartekbugala/BannerLink-Admin-Panel-Layout.git"
  },
  "bugs": {
    "url": "https://git@github.com:bartekbugala/BannerLink-Admin-Panel-Layout.git/issues"
  },
  "homepage": "https://git@github.com:bartekbugala/BannerLink-Admin-Panel-Layout.git#readme"
}
